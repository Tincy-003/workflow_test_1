<mvc:View
    controllerName="ApproveForm.approveform.controller.App"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <App id="app">
        <pages>
            <Page
                id="_IDGenPage1"
                showHeader="false"
                showFooter="false"
            >
                <content>
                    <Label
                        id="_IDGenLabel1"
                        text="{i18n>BusinessPartnerGrouping}"
                        required="true"
                    />
                    <Input
                        id="_IDGenInput1"
                        value="{context>/BPCreationRequest/BusinessPartnerGrouping}"
                        editable="false"
                    />

                    <Label
                        id="_IDGenLabel2"
                        text="{i18n>BusinessPartnerCategory}"
                        required="true"
                    />
                    <Input
                        id="_IDGenInput2"
                        value="{context>/BPCreationRequest/BusinessPartnerCategory}"
                        editable="false"
                    />
                    <Label
                        id="_IDGenLabel3"
                        text="{i18n>OrganizationBPName1}"
                        required="true"
                    />
                    <Input
                        id="_IDGenInput3"
                        value="{context>/BPCreationRequest/OrganizationBPName1}"
                        editable="false"
                    />
                    <Label
                        id="_IDGenLabel4"
                        text="{i18n>SearchTerm1}"
                        required="true"
                    />
                    <Input
                        id="_IDGenInput4"
                        value="{context>/BPCreationRequest/SearchTerm1}"
                        editable="false"
                    />
                    <Label
                        id="_IDGenLabel5"
                        text="{i18n>BusinessPartnerType}"
                        required="true"
                    />
                    <Input
                        id="_IDGenInput5"
                        value="{context>/BPCreationRequest/BusinessPartnerType}"
                        editable="false"
                    />
                </content>
            </Page>
        </pages>
    </App>
</mvc:View>
